= form_for @project do |f|
  -if @project.errors.any?
    #error_explanation
      %h2= "#{pluralize(@project.errors.count, "error")} prohibited this project from being saved:"
      %ul
        - @project.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :titulo
    = f.text_field :titulo
  .field
    = f.label :descripcion
    = f.text_field :descripcion
  .field
    = f.label :inicio
    = f.date_select :inicio
  .field
    = f.label :fin
    = f.date_select :fin
  .field
    = f.label :projectstatus_id
    = f.number_field :projectstatus_id
  .field
    = f.label :user_id
    = f.number_field :user_id
  .actions
    = f.submit 'Save'
